#include <iostream>
#include "LList2.hpp"
using namespace std;

#def MAX 8
int main ()
{

  LList2<int> L1;
  for (int i = 0; i < MAX; i++)
    if ( i % 2)
      L1.InsertFirst(i);
    else
      L1.Insertlast(i);
  cout << "L1 in order: ";
  cout << L1 << endl;
  cout << "Testing begin, end, post++ and *\n";
  for (LList2<int>::Iterator itr = L1.begin (); itr != L1.end(); itr++)
    {
      cout << "Value: " << * itr << endl;
      return 0;
    }
} 
